<?xml version="1.0" encoding="UTF-8"?>
<!--

  YADS - OpenURL Example 1b (see ex-1.xml for example case)

  ==

  a) Unencoded OpenURL (ignore whitespace):
  
  http://openurl.org/resolver?
    & ref_valfmt = article
    & aulast = caplan
    & aufirst = priscilla
    & issn = 10829873
    & volume = 5
    & issue = 7
    & atitle = Reference Linking for Journal Articles

-->
<yads>
<nest>
<!-- OpenURL Resolver + Query -->
<resource>http://openurl.org/resolver?</resource>
<collection>
<item>
<!-- Referent -->
<data>article</data>
<property type="type">ref_valfmt</property>
</item>
<item>
<!-- Referent -->
<data>caplan</data>
<property type="type">aulast</property>
</item>
<item>
<!-- Referent -->
<data>priscilla</data>
<property type="type">aufirst</property>
</item>
<item>
<!-- Referent -->
<data>10829873</data>
<property type="type">issn</property>
</item>
<item>
<!-- Referent -->
<data>5</data>
<property type="type">volume</property>
</item>
<item>
<!-- Referent -->
<data>7</data>
<property type="type">issue</property>
</item>
<item>
<!-- Referent -->
<data>Reference Linking for Journal Articles</data>
<property type="type">atitle</property>
</item>
</collection>
<property type="type">OpenURL</property>
</nest>
</yads>
