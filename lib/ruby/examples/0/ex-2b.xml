<?xml version="1.0" encoding="UTF-8"?>
<!--

  YADS - OpenURL Example 2b (see ex-2.xml for example case)

  ==

  a) Unencoded OpenURL (ignore whitespace):
  
  http://openurl.org/resolver?
    & ref_id = isbn:1861004516
    & req_id = local:104-0011434-4639158
    & rfr_id = dns:amazon.com
    & rfe_id = http:www.amazon.com/exec/obidos/ASIN/1861004516
    & srv_id = local:addToCart
    & adm_tim = 2002-03-20T13:05:54Z 

-->
<yads>
<nest>
<!-- OpenURL Resolver + Query -->
<resource>http://openurl.org/resolver?</resource>
<collection>
<item>
<!-- Referent -->
<data>isbn:1861004516</data>
<property type="type">ref_id</property>
</item>
<item>
<!-- Requester -->
<data>local:104-0011434-4639158</data>
<property type="type">req_id</property>
</item>
<item>
<!-- Referer -->
<data>dns:amazon.com</data>
<property type="type">rfr_id</property>
</item>
<item>
<!-- Refererring-Entity -->
<resource>http://www.amazon.com/exec/obidos/ASIN/1861004516</resource>
<property type="type">rfe_id</property>
</item>
<item>
<!-- Service-Type -->
<data>local:addtoCart</data>
<property type="type">srv_id</property>
</item>
<item>
<!-- Admin -->
<data>2002-03-20T13:05:54Z</data>
<property type="type">adm_tim</property>
</item>
</collection>
<property type="type">OpenURL</property>
</nest>
</yads>
