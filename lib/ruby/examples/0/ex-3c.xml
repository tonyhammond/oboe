<?xml version="1.0" encoding="UTF-8"?>
<!--

  YADS - OpenURL Example 3c (see ex-3.xml for example case)

  ==

  a) Unencoded OpenURL (ignore whitespace):
  
  http://openurl.org/resolver?
    & ref_id = pmid:11823195
    & req_id = mailto:joe.doe@greatjob.org
    & rfe_id = pmid:11823195
    & rfr_id = dns:ncbi.nlm.nih.gov:pubmed 

-->
<yads>
<nest>
<!-- OpenURL Resolver + Query -->
<resource>http://openurl.org/resolver?</resource>
<collection>
<item>
<!-- Referent -->
<data>pmid:11823195</data>
<property type="type">ref_id</property>
</item>
<item>
<!-- Requester -->
<resource>mailto:joe.doe@greatjob.org</resource>
<property type="type">req_id</property>
</item>
<item>
<!-- Referring-Entity -->
<data>pmid:11823195</data>
<property type="type">rfe_id</property>
</item>
<item>
<!-- Referer -->
<data>dns:ncbi.nlm.nih.gov:pubmed</data>
<property type="type">rfr_id</property>
</item>
</collection>
<property type="type">OpenURL</property>
</nest>
</yads>
