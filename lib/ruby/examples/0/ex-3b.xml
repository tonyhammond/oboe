<?xml version="1.0" encoding="UTF-8"?>
<!--

  YADS - OpenURL Example 3b (see ex-3.xml for example case)

  ==

  a) Unencoded OpenURL (ignore whitespace):
  
  http://openurl.org/resolver?
    & ref_valfmt = article
    & aulast = Sturino
    & auinit = JM
    & stitle = Appl Environ Microbiol
    & volume = 68
    & issue = 2
    & date = 2002-02 

-->
<yads>
<nest>
<!-- OpenURL Resolver + Query -->
<resource>http://openurl.org/resolver?</resource>
<collection>
<item>
<!-- Referent -->
<data>article</data>
<property type="type">ref_valfmt</property>
</item>
<item>
<!-- Referent -->
<data>Sturino</data>
<property type="type">aulast</property>
</item>
<item>
<!-- Referent -->
<data>JM</data>
<property type="type">auinit</property>
</item>
<item>
<!-- Referent -->
<data>Appl Eviron Microbiol</data>
<property type="type">stitle</property>
</item>
<item>
<!-- Referent -->
<data>68</data>
<property type="type">volume</property>
</item>
<item>
<!-- Referent -->
<data>2</data>
<property type="type">issue</property>
</item>
<item>
<!-- Referent -->
<data>2002-02</data>
<property type="type">date</property>
</item>
</collection>
<property type="type">OpenURL</property>
</nest>
</yads>
