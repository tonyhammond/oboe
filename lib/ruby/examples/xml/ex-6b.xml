          </item>
        </collection>
      </item>
      <item>
        <!-- Context-Object -->
        <property type="type">oboe:context-object</property>
        <collection>
          <item>
            <!-- Referent -->
            <property type="type">oboe:referent</property>
            <collection>
              <item>
                <!-- Metadata-by-val -->
                <property type="type">oboe:metadata-by-val</property>
                <collection>
                </collection>
              </item>
            </collection>
          </item>
          <item>
            <!-- Referring-entity -->
            <property type="type">oboe:referring-entity</property>
            <collection>
              <item>
                <!-- Identifier -->
                <property type="type">oboe:identifier</property>
                <data>uri:urn:ISBN:0262531283</data>
              </item>
            </collection>
          </item>
        </collection>
      </item>
    </collection>
  </nest>
    </collection>
  </nest>
