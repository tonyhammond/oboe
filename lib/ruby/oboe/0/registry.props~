# Keywords (v.0.1)

registry.keys_01 = \
  registry.key_01.sid registry.key_01.id registry.key_01.pid \
  registry.key_01.genre registry.key_01.aulast registry.key_01.aufirst \
  registry.key_01.auinit registry.key_01.auinit1 registry.key_01.auinitm \
  registry.key_01.coden registry.key_01.issn registry.key_01.eissn \
  registry.key_01.isbn registry.key_01.title registry.key_01.stitle \
  registry.key_01.atitle registry.key_01.volume registry.key_01.part \
  registry.key_01.issue registry.key_01.spage registry.key_01.epage \
  registry.key_01.pages registry.key_01.artnum registry.key_01.sici \
  registry.key_01.bici registry.key_01.ssn registry.key_01.quarter \
  registry.key_01.date

# Origin-Description
registry.key_01.sid     = sid

# Global-Identifer-Zone
registry.key_01.id      = id

# Local-Identifer-Zone
registry.key_01.pid     = pid

# Object-Metadata-Zone Meta-Tags
registry.key_01.genre   = genre
