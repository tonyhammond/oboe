# Schemas (v.1.0)

registry.schemas = \
  registry.schema.book \
  registry.schema.book_component \
  registry.schema.conf_proc \
  registry.schema.conf_paper \
  registry.schema.dissertation \
  registry.schema.journal \
  registry.schema.jissue \
  registry.schema.jarticle \
  registry.schema.patent \
  registry.schema.tech_report

# registry.schema.book = doi:10.niso/book
registry.schema.book = book
registry.schema.book_keys = \
  au aufirst aulast btitle date isbn org place pub title tpages

# registry.schema.book_component = doi:10.niso/book_component
registry.schema.book_component = book_component
registry.schema.book_component_keys \
  artnum au aufirst aulast chron coden date eissn epage issn issue \
  jtitle org pages part sici spage stitle title volume

# registry.schema.conf_proc = doi:10.niso/conf_proc
registry.schema.conf_proc = conf_proc
registry.schema.conf_proc_keys \
  artnum au aufirst aulast chron coden date eissn epage issn issue \
  jtitle org pages part sici spage stitle title volume

# registry.schema.conf_paper = doi:10.niso/conf_paper
registry.schema.conf_paper = conf_paper
registry.schema.conf_paper_keys \
  artnum au aufirst aulast chron coden date eissn epage issn issue \
  jtitle org pages part sici spage stitle title volume

# registry.schema.dissertation = doi:10.niso/dissertation
registry.schema.dissertation = dissertation
registry.schema.dissertation_keys \
  artnum au aufirst aulast chron coden date eissn epage issn issue \
  jtitle org pages part sici spage stitle title volume

# registry.schema.journal = doi:10.niso/journal
registry.schema.journal = journal
registry.schema.journal_keys \
  coden eissn issn jtitle stitle title

# registry.schema.jissue = doi:10.niso/jissue
registry.schema.jissue = jissue
registry.schema.jissue_keys \
  chron coden date eissn issn issue jtitle part sici stitle title volume

# registry.schema.jarticle = doi:10.niso/jarticle
registry.schema.jarticle = jarticle
registry.schema.jarticle_keys \
  artnum au aufirst aulast chron coden date eissn epage issn issue \
  jtitle org pages part sici spage stitle title volume

# registry.schema.patent = doi:10.niso/tech_report
registry.schema.patent = tech_report
registry.schema.patent_keys \
  artnum au aufirst aulast chron coden date eissn epage issn issue \
  jtitle org pages part sici spage stitle title volume

# registry.schema.tech_report = doi:10.niso/tech_report
registry.schema.tech_report = tech_report
registry.schema.tech_report_keys \
  artnum au aufirst aulast chron coden date eissn epage issn issue \
  jtitle org pages part sici spage stitle title volume

