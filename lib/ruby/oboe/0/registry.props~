# Keywords (v.1.0)

registry.keys = \
  registry.key.adm_set registry.key.adm_tim registry.key.adm_ver \
  registry.key.ctx_ptr registry.key.ctx_pid registry.key.ref_id \
  registry.key.ref_valfmt registry.key.ref_reffmt registry.key.ref_ptr \
  registry.key.ref_pid registry.key.rfr_id registry.key.rfr_reffmt \
  registry.key.rfr_ptr registry.key.rfr_pid registry.key.rfe_id \
  registry.key.rfe_reffmt registry.key.rfe_ptr registry.key.rfe_pid \
  registry.key.req_id registry.key.req_reffmt registry.key.req_ptr \
  registry.key.req_pid registry.key.res_id registry.key.res_reffmt \
  registry.key.res_ptr registry.key.res_pid registry.key.srv_id \
  registry.key.srv_reffmt registry.key.srv_ptr registry.key.srv_pid

# Admin
registry.key.adm_set    = adm_set
registry.key.adm_tim    = adm_tim
registry.key.adm_ver    = adm_ver

# ContextObject
registry.key.ctx_ptr    = ctx_ptr
registry.key.ctx_pid    = ctx_pid

# Referent
registry.key.ref_id     = ref_id
registry.key.ref_valfmt = ref_valfmt
registry.key.ref_reffmt = ref_reffmt
registry.key.ref_ptr    = ref_ptr
registry.key.ref_pid    = ref_pid

# Referrer
registry.key.rfr_id     = rfr_id
registry.key.rfr_reffmt = rfr_reffmt
registry.key.rfr_ptr    = rfr_ptr
registry.key.rfr_pid    = rfr_pid

# Referring-Entity
registry.key.rfe_id     = rfe_id
registry.key.rfe_reffmt = rfe_reffmt
registry.key.rfe_ptr    = rfe_ptr
registry.key.rfe_pid    = rfe_pid

# Requester
registry.key.req_id     = req_id
registry.key.req_reffmt = req_reffmt
registry.key.req_ptr    = req_ptr
registry.key.req_pid    = req_pid

# Resolver
registry.key.res_id     = res_id
registry.key.res_reffmt = res_reffmt
registry.key.res_ptr    = res_ptr
registry.key.res_pid    = res_pid

# Service-Type
registry.key.srv_id     = srv_id
registry.key.srv_reffmt = srv_reffmt
registry.key.srv_ptr    = srv_ptr
registry.key.srv_pid    = srv_pid

