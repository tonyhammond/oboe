# Namespace identifiers (v.1.0)

registry.nids = \
  registry.nid._ri registry.nid.ori registry.nid.uri

registry.nid._ri = _ri
registry.nid.ori = ori
registry.nid.uri = uri

# Generic resource identifier schemes
registry.nid._ri.schemes = \
  registry.nid._ri.schemes.oboe registry.nid._ri.schemes.xref

registry.nid._ri.schemes.oboe = oboe
registry.nid._ri.schemes.xref = xref

# OpenURL resource identifier schemes
registry.nid.ori.schemes = \
  registry.nid.ori.schemes.bibcode registry.nid.ori.schemes.doi \
  registry.nid.ori.schemes.oai registry.nid.ori.schemes.pmid \
  registry.nid.ori.schemes.dbid registry.nid.ori.schemes.issn \
  registry.nid.ori.schemes.isbn

registry.nid.ori.schemes.bibcode = bibcode
registry.nid.ori.schemes.doi = doi
registry.nid.ori.schemes.oai = oai
registry.nid.ori.schemes.pmid = pmid
registry.nid.ori.schemes.dbid = dbid
registry.nid.ori.schemes.issn = issn
registry.nid.ori.schemes.isbn = isbn

# Uniform resource identifier schemes
registry.nid.uri.schemes = \
  registry.nid.uri.schemes.ftp registry.nid.uri.schemes.http \
  registry.nid.uri.schemes.mailto

registry.nid.uri.schemes.ftp = ftp
registry.nid.uri.schemes.http = http
registry.nid.uri.schemes.mailto = mailto

